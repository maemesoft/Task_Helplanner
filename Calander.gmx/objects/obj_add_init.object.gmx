// Loop through all the weeks
for (i=0; i&lt;6; i+=1) {
    // Loop through each day in the week
    for (ii=0; ii&lt;7; ii+=1) {
        // Get the value we are seeking from the grid
        to_write = string(ds_grid_get(calander_grid,ii,i));
        
        // If the number is a single digit, add a space to the front so everything lines up nice
        if (string_length(to_write) == 1) {
            to_write = " "+to_write;
        }
        
        // Add a space to the end so there is a space between the numbers
        calander_string += to_write+" ";
    }
    // Add a newline at the end of the week
    calander_string += "#";
}

// This system works using a fixed width font (important!). If you're going to use
// a font that doesn't have a fixed width, be prepared to run more calculations on where to draw
draw_set_font(courier);

// Draw the calander to the screen

