            <string>// Calander grid that stores the date data
calander_grid = ds_grid_create(7,6);

// Set the default grid value to a space
ds_grid_clear(calander_grid," ");

// The day of the week the first of the month was on (0 based)
weekday = date_get_weekday(date_current_datetime()-current_day);

// Get the number of days in the current month
days_in_month = date_days_in_month(date_current_datetime());

// The current week that is being written (0 based)
week = 0;

for (i=1; i&lt;=days_in_month; i+=1) {
    ds_grid_set(calander_grid,weekday,week,i);
    
    // Add a weekday
    weekday +=1;
    
    // If it is the 8th day of the week (recall that it is 0 based)...
    if (weekday == 7) {
        // Move to the next week
        week+=1;
        // Set the weekday to Sunday
        weekday = 0;
    }
}
</string>
